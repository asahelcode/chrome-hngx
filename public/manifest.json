{
  "manifest_version": 3,
  "name": "HelpMeOut",
  "description": "Records a user's screen and send the recorded content to a specific online location",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
        "32": "icon32.png"
    }
  },
    "permissions": [
        "activeTab",
        "desktopCapture",
        "storage"
        ]
}